{
  "name": "websheets",
  "version": "0.1.1",
  "description": "A spreadsheet component for the web",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir build --source-maps --no-babelrc --presets es2015",
    "prepublish": "rm -rf build/* && npm run build && cp -a src/compiler/static build/compiler/static",
    "watch": "fswatch -0 src | xargs -0 -n 1 -I {} npm run web",
    "web": "./node_modules/.bin/webpack --config=webpack.config.js --progress"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattbasta/websheets.git"
  },
  "keywords": [
    "spreadsheet",
    "table",
    "formula",
    "expression",
    "excel"
  ],
  "author": "Matt Basta <me@mattbasta.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattbasta/websheets/issues"
  },
  "homepage": "https://github.com/mattbasta/websheets"
}
